---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xml:lang="en-US" xmlns:media="http://search.yahoo.com/mrss/"  xmlns="http://www.w3.org/2005/Atom">
  <title>ZTNOTE</title>
  <link type="text/html" href="http://{{ site.domain }}" rel="alternate"/>
  <link type="application/atom+xml" href="http://{{ site.domain }}/atom.xml" rel="self"/>
  <updated>{{site.time | date_to_xmlschema }}</updated>
  <id>http://{{ site.domain }}/</id>
  <author>
    <name>{{ site.author }}</name>
    <email>{{ site.email }}</email>
  </author>
  {% for post in site.posts limit: 10 %}
  <entry>
    <title>{{ post.title }}</title>
    <link href="http://{{ site.domain }}{{ post.url }}"/>
    <updated>{{post.date | date_to_xmlschema }}</updated>
    <id>http://{{ site.domain }}{{ post.id }}</id>
    <content type="html">{{ post.content | xml_escape }}</content>
  </entry>
  {% endfor %}
</feed>
